{
    "name": "purewater2011/hyperf-elasticsearch",
    "description": "The Elasticsearch Driver for Hyperf Scout",
    "keywords": [
        "elastic",
        "elasticsearch",
        "driver",
        "engine",
        "hyperf",
        "scout",
        "search"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "eric",
            "email": "eric@qq.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "hyperf/elasticsearch": "~2.2.0",
        "hyperf/paginator": "2.2.0",
        "hyperf/database": "2.2.0",
        "symfony/console": "^5.4",
        "hyperf/command": "2.2.0",
        "hyperf/devtool": "2.2.0"
    },
    "autoload": {
        "psr-4": {
            "PurewaterEs\\ScoutElastic\\": "src/",
            "PurewaterEs\\ScoutElastic\\Tests\\": "tests/"
        }
    },
    "extra": {
        "hyperf": {
            "config": "PurewaterEs\\ScoutElastic\\ConfigProvider"
        }
    },
    "require-dev": {
        "swoole/ide-helper": "^6.0"
    }
}
